{
  "name": "vue-dynamic-reconfigure",
  "version": "0.1.4",
  "description": "Vue components for ROS dynamic_reconfigure",
  "homepage": "https://github.com/LdwgWffnschmdt/vue-dynamic-reconfigure",
  "repository": {
    "type": "git",
    "url": "https://github.com/LdwgWffnschmdt/vue-dynamic-reconfigure"
  },
  "keywords": [
    "vue",
    "vuejs",
    "ros",
    "dynamic_reconfigure"
  ],
  "author": "Ludwig Waffenschmidt <ludwig.waffenschmidt@outlook.com>",
  "main": "dist/vue-dynamic-reconfigure.ssr.js",
  "module": "dist/vue-dynamic-reconfigure.esm.js",
  "unpkg": "dist/vue-dynamic-reconfigure.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/App.vue",
    "!src/main.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./src/main.js",
    "build-demo": "vue-cli-service build --dest demo ./src/main.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "material-design-icons-iconfont": "^4.0.5",
    "core-js": "^3.4.4",
    "roboto-fontface": "*",
    "roslib": "^1.0.1",
    "vue": "^2.6.11",
    "vuetify": "^1.5.22"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "rollup": "^1.26.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "5.1.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.2.1"
  }
}
